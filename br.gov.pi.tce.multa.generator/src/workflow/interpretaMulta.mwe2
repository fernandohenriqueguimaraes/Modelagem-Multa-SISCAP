module workflow.interpretaMulta

var model = "br.gov.pi.tce.multa.generator/src/default.multa"
 
Workflow {
   bean = org.eclipse.emf.mwe.utils.StandaloneSetup {
       platformUri = ".."
       registerGeneratedEPackage = "br.gov.pi.tce.multa.MultaPackage"
   }
 
   component = org.eclipse.emf.mwe.utils.Reader {
       uri = "platform:/resource/${model}"
       modelSlot = "model"
   }
   component = multa.Executa {}
}
